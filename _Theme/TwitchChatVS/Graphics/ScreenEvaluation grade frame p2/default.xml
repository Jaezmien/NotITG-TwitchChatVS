<ActorFrame> <children>
	<Layer Type="ActorFrame" OnCommand="x,-70;y,-97"> <children>
		<Layer Type="BitmapText" Font="_misoreg white" Text="FANTASTIC"	OnCommand="y,27*0;zoom,0.35;horizalign,right;ShadowLength,0" />
