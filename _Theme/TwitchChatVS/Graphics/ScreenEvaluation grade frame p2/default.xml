		<Layer Type="BitmapText" Font="_misoreg white" Text="WAY OFF"	OnCommand="y,27*4;zoom,0.35;horizalign,right;ShadowLength,0" InitCommand="%function(self) if not WayOffs() then self:diffuse(.5,.5,.5,.5) end end" />
